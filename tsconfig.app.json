{
  "extends": "@vue/tsconfig/tsconfig.web.json",
  "include": [
    "env.d.ts",
    "src/**/*",
    "src/**/*.vue",
    "node_modules/cypress/types/cypress-global-vars.d.ts"
  ],
  "exclude": [
    "src/**/__tests__/*",
    "cypress/**/*.cy.ts",
    "src/**/*.cy.ts",
  ],
  "compilerOptions": {
    "composite": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"]
    }
  }
}
